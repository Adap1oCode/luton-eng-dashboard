{
  "name": "studio-admin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --turbo",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:vercel": "vercel build",
    "preview:vercel": "vercel dev --prebuilt",
    "start": "next start -p 3000",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky && husky install",
    "validate:tiles": "tsc --project tsconfig.validate.json && node dist/lib/validate-tiles.js",
    "verify": "ts-node -r tsconfig-paths/register -r dotenv/config -P tsconfig.scripts.json scripts/verify.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "generate:page": "node scripts/generate-resource-page.js",
    "performance:test": "vitest run src/tests/performance/",
    "performance:monitor": "echo 'Visit http://localhost:3000/performance to view performance dashboard'",
    "build:verify": "vitest run src/tests/integration/build-verification.spec.ts",
    "build:simple": "vitest run src/tests/integration/simple-build-verification.spec.ts",
    "build:e2e": "vitest run src/tests/integration/end-to-end-build.spec.ts",
    "build:performance": "vitest run src/tests/integration/performance-build.spec.ts",
    "build:full": "npm run build && npm run build:verify && npm run build:e2e && npm run build:performance",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:smoke": "playwright test -g @smoke",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0 || true",
    "ci:verify": "pnpm typecheck && pnpm lint && pnpm build && node scripts/require-docs.mjs && start-server-and-test start http://localhost:3000 'pnpm test && pnpm test:e2e:smoke'",
    "lint:fix": "next lint --fix",
    "gen:schemas": "ts-node --esm scripts/gen-schemas-from-registry.ts",
    "gen:openapi": "ts-node --esm scripts/gen-openapi-from-registry.ts",
    "setup:logging": "node scripts/setup-logging.mjs",
    "check:logging": "node scripts/setup-logging.mjs --check"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
